 // 3. 普通遍历\n              // for (let item of this.cartList) {\n              //     if (!item.checked) {\n              //         return false\n              //     }\n              // }\n              //\n              // return true\n          }\n      },\n      methods: {\n          checkClick() {\n              if (this.isSelectAll) { // 全部选中\n                  this.cartList.forEach(item => item.checked = false)\n              } else { // 部分或全部不选中\n                  this.cartList.forEach(item => item.checked = true)\n              }\n          },\n\n          // this.cartList.forEach(item => item.checked = !this.isSelectAll)\n\n          calcClick() {\n              this.$toast.show('请选择购买的商品', 2000)\n              this.$toast.show('请选择购买的商品', 2000)\n          }\n      },\n\n  }\n</script>\n\n<style scoped>\n  .bottom-bar {\n    height: 40px;\n    background-color: #eee;\n    position: relative;\n    line-height: 40px;\n    display: flex;\n    font-size: 14px;\n  }\n\n  .check-content {\n    display: flex;\n    align-items: center;\n    margin-left: 10px;\n    width: 60px;\n  }\n\n  .check-button {\n    width: 20px;\n    height: 20px;\n    line-height: 20px;\n    margin-right: 5px;\n  }\n\n  .price {\n    margin-left: 30px;\n    flex: 1;\n  }\n\n  .calculate {\n    width: 90px;\n    background-color: red;\n    color: #fff;\n    text-align: center;\n  }\n</style>\n"],"sourceRoot":"src/views/cart/childComps"}]}